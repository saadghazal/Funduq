@model IEnumerable<funduq.Models.ContactU>

@{
    ViewData["Title"] = "Index";
    Layout = "/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ContactUsPhoneNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ContactUsEmail)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ContactUsAddress)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ContactUsOpenDays)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ContactUsOpenHours)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ContactUsPhoneNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ContactUsEmail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ContactUsAddress)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ContactUsOpenDays)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ContactUsOpenHours)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ContactId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ContactId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ContactId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
